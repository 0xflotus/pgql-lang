  prettyprint-Labels :
    Labels(t1__) -> [ H(
                        [SOpt(HS(), "0")]
                      , [S(":"), t1__']
                      )
                    ]
    with t1__' := <pp-one-Z(prettyprint-AltLit)> t1__

  is-Labels =
    ?Labels(_)

  prettyprint-Ids :
    Ids(t1__) -> [ H(
                     [SOpt(HS(), "0")]
                   , [S("@"), t1__']
                   )
                 ]
