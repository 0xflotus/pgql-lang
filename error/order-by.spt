module order-by

language PGQL

test Order by non-group key [[
SELECT n.age
WHERE
  n
GROUP BY n.age
ORDER BY n.name
]] / Cannot order by non-group key variable without using aggregation /

test Nested aggregation [[
SELECT AVG(n.age)
WHERE
  n
ORDER BY AVG(MIN(n.age))
]] / Nested aggregation is not allowed /
