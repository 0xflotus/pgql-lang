module PGQL.main

imports

	PGQL-Menus
	PGQL-Colorer
	PGQL-Completions
	PGQL-Folding
	PGQL-References
	PGQL-Syntax
	PGQL-Views

  // src-gen/completions/GMQL-esv

language General properties
                  
  name          : PGQL
  id            : oracle.pgql.lang
  extends       : Root
                  
  description   : "Spoofax/IMP-generated editor for the GMQL language"
  url           : http://strategoxt.org
                  
  extensions    : pgql
  table         : include/PGQL.tbl
  start symbols : Start
  
  provider      : include/pgql.jar
  provider      : include/pgql-java.jar
  
  observer      : editor-analyze
  
  on save       : editor-save
  
  jvm opts      : -Xmx512m
