MATCH
  {
    MATCH
      (person:Person@12345)-[:KNOWS]{1..2}-(friend:Person)
      person != friend
    SELECT DISTINCT friend
  }
  friend <-[:HAS_CREATOR]- (post:Post) -[:HAS_TAG]-> (knownTag:Tag WITH ~name = 'TagName')
  post-[:HAS_TAG]->(tag:Tag WITH tag != knownTag)
SELECT tag.name, COUNT(post)
GROUP BY tag
ORDER BY DESC(COUNT(post)), ASC(tag.name)
LIMIT 10